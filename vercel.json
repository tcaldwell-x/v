{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm cache clean --force && npm install --no-package-lock --registry=https://registry.npmjs.org/",
  "framework": "nextjs",
  "regions": ["sfo1"],
  "routes": [
    {
      "src": "/_next/static/(.*)",
      "dest": "/_next/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
} 